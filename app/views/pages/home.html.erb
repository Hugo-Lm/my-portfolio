<div class="main-container">
  <div class="block-top">
    <div class="block-top-left">
      <h1>Hi, I'm Hugo</h1>
    </div>
    <div class="block-top-center">
    </div>
    <div class="block-top-right">
    </div>
  </div>
  <div class="block-middle">
    <div class="block-middle-left">

    </div>
    <div class="block-middle-center">
      <%= image_tag("profil_bw", class: "profil-picture")%>
    </div>
    <div class="block-middle-right">

    </div>
  </div>
  <div class="block-bottom">
    <div class="block-bottom-left">
    </div>
    <div class="block-bottom-center">
    </div>
    <div class="block-bottom-right">
      <h2>Full-stack</h2>
      <h2>web developper</h2>
    </div>
  </div>
  <div class="projects-title">
    <h2>My projects</h2>
  </div>
  <div class="projects">
    <div class="projects-block">
      <div class="project-card">
        <h2>Garden Party</h2>
      </div>
      <div class="project-card">
        <h2>Share Voisins</h2>
      </div>
      <div class="project-card">
        <h2>Cocktail's fabrik</h2>
      </div>
    </div>

    <div id="carouselGarden" class="carousel slide" data-ride="carousel">
      <ol class="carousel-indicators">
        <li data-target="#carouselGarden" data-slide-to="0" class="active"></li>
        <li data-target="#carouselGarden" data-slide-to="1"></li>
        <li data-target="#carouselGarden" data-slide-to="2"></li>
      </ol>
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="garden1" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="garden2" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="garden3" class="d-block w-100" alt="...">
        </div>
      </div>
      <a class="carousel-control-prev" href="#carouselGarden" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>
      <a class="carousel-control-next" href="#carouselGarden" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>
    </div>

    <div class="green-background-projects">

    </div>
  </div>
